{
  "name": "flyswap",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "max build",
    "build:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max build",
    "build:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max build",
    "build:pro": "cross-env REACT_APP_ENV=pro UMI_ENV=dev max build",
    "build:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max build",
    "dev": "max dev",
    "format": "prettier --cache --write .",
    "postinstall": "max setup",
    "prepare": "husky install",
    "setup": "max setup",
    "start": "npm run dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:pro": "cross-env REACT_APP_ENV=pro UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^2.0.1",
    "@umijs/max": "^4.0.25",
    "antd": "^4.20.7",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^2",
    "prettier-plugin-packagejson": "^2",
